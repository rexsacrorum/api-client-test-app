@model List<CeTestApp.Web.Domain.Contracts.OrderContract>

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">ChannelName</th>
        <th scope="col">Order lines</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.Id</td>
            <td>@order.ChannelName</td>
            <td>
                @await Html.PartialAsync("_orderLines", order.Lines)
            </td>
        </tr>
    }
    </tbody>
</table>
